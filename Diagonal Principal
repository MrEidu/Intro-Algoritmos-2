#include <stdio.h>
#include
using namespace std;

int main () {
    int n, m;
    Matriz[n][m];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            if (i==j)
                cout << Matriz[i][j] << endl;
        }
    }
    return 0;
}

